#!/bin/bash
# LFS Guard pre-commit hook

# Abort commit if git lfs has tracked files
if git lfs ls-files | grep -q .; then
    echo "âŒ LFS usage is prohibited in this repo." >&2
    exit 1
fi

exit 0

